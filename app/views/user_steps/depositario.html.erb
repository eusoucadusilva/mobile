<%= render layout: 'form' do |f| %>
<fieldset>
  <legend>Fiel Depositário</legend>
  <%= f.input :depositary %>
  <%= f.input :depositary_address %>
  <%= f.input :depositary_city %>
  <%= f.input :depositary_cep %>
  <%= f.input :depositary_district %>
  <%= f.input :depositary_state %>
  <%= f.input :depositary_cnpj %>
</fieldset>

<fieldset>
  <legend>Descrição dos Bens</legend>
  <%= f.input :goods, :collection => User::GOODS, :prompt => "Por favor selecione" %>
</fieldset>
<% end %>